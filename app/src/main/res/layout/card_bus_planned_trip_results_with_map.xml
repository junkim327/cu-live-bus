<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white">

    <ImageView
        android:id="@+id/image_bus_icon_planned_trip_results_with_map_bus_card"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/text_bus_minutes_planned_trip_results_with_map_bus_card"
        app:layout_constraintEnd_toEndOf="@+id/text_bus_minutes_planned_trip_results_with_map_bus_card"
        app:layout_constraintStart_toStartOf="@+id/text_bus_minutes_planned_trip_results_with_map_bus_card"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.6"
        app:layout_constraintVertical_chainStyle="packed"
        app:srcCompat="@drawable/ic_directions_bus_black_24dp" />

    <TextView
        android:id="@+id/text_start_point_name_planned_trip_results_with_map_bus_card"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="32dp"
        android:fontFamily="@font/oxygen_bold"
        android:maxWidth="300dp"
        android:maxLines="2"
        android:text="@string/no_info"
        android:textColor="@color/textMainColor"
        android:textDirection="ltr"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/text_bus_name_planned_trip_results_with_map_bus_card"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guideline5"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="packed"
        tools:text="Board Munjeong The Legal Profession Danji.Geonyeong Apartment" />

    <TextView
        android:id="@+id/text_bus_name_planned_trip_results_with_map_bus_card"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="32dp"
        android:fontFamily="@font/oxygen_bold"
        android:text="@string/no_info"
        android:textColor="@color/textMainColor"
        app:layout_constraintBottom_toTopOf="@+id/text_end_point_name_planned_trip_results_with_map_bus_card"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guideline5"
        app:layout_constraintTop_toBottomOf="@+id/text_start_point_name_planned_trip_results_with_map_bus_card"
        tools:text="220S Illini" />

    <TextView
        android:id="@+id/text_end_point_name_planned_trip_results_with_map_bus_card"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="16dp"
        android:fontFamily="@font/oxygen_bold"
        android:maxWidth="300dp"
        android:maxLines="2"
        android:text="@string/no_info"
        android:textColor="@color/textMainColor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guideline5"
        app:layout_constraintTop_toBottomOf="@+id/text_bus_name_planned_trip_results_with_map_bus_card"
        tools:text="Arrive at somewhere" />

    <TextView
        android:id="@+id/text_bus_minutes_planned_trip_results_with_map_bus_card"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="8dp"
        android:fontFamily="@font/oxygen"
        android:text="@string/no_info"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/guideline5"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/image_bus_icon_planned_trip_results_with_map_bus_card"
        tools:text="6min" />

    <View
        android:id="@+id/view"
        style="@style/Divider"
        android:layout_width="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/text_end_point_name_planned_trip_results_with_map_bus_card" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="115dp" />
</androidx.constraintlayout.widget.ConstraintLayout>